{
  "expo": {
    "name": "timeToReactNative",
    "slug": "timeToReactNative",
    "author": "kisekiremi",
    "version": "1.0.0",
    "userInterfaceStyle": "light",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/static/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apps.app1"
    },
    "android": {
      "bundleIdentifier": "com.apps.app1",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/static/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/static/favicon.png"
    }
  }
}
